{
    "name": "kalprajsolutions/laravel-onedrive-filesystem",
    "description": "OneDrive filesystem driver for Laravel using Microsoft Graph API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kalprajsolutions",
            "email": "dev@kalprajsolutions.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/filesystem": "^10.0|^11.0|^12.0",
        "league/flysystem": "^3.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "KalprajSolutions\\LaravelOnedriveFilesystem\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KalprajSolutions\\LaravelOnedriveFilesystem\\OneDriveServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
